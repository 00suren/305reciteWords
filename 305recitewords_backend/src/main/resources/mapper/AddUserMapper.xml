<!DOCTYPE mapper PUBLIC "-//mybatis .org//DTD Mapper 3.0//EN" "http://mybatis. org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.suren.dao.AddUserInfoDao">
    <insert id="addUserInfo" parameterType="com.suren.entities.User" useGeneratedKeys="true" keyProperty="wxid">
        insert into user(name,wxid,avatar) values(#{name},#{wxid},#{avatar})
    </insert>
</mapper>