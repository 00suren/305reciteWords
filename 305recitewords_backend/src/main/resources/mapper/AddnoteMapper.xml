        <!DOCTYPE mapper PUBLIC "-//mybatis .org//DTD Mapper 3.0//EN" "http://mybatis. org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.suren.dao.AddNoteDao">
<insert id="addNote" parameterType="com.suren.entities.Note" useGeneratedKeys="true" keyProperty="noteid">
    insert into note(notecontent,wxid,wordid,wordcontent) values(#{note.notecontent},#{note.wxid},#{note.wordid},#{note.wordcontent})
</insert>
</mapper>